		"express": "^4.18.2",
		"express-validator": "^7.0.1",
		"jsonwebtoken": "^9.0.2",
		"mysql2": "^3.6.5",
		"nodemailer": "^6.9.8",
		"pug": "^3.0.2",
		"sequelize": "^6.35.2"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.16",
		"concurrently": "^8.2.2",
